    "babel-preset-stage-2": "^6.16.0",
    "babel-register": "^6.16.3",
    "chai": "^3.5.0",
    "eslint": "^4.7.2",
    "eslint-config-stripes": "^0.0.1",
    "mocha": "^3.1.0",
    "rimraf": "^2.5.4",
    "webpack": "^3.6.0"
